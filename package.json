{
  "name": "@spendlore/learning",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "description": "Effect TypeScript learning materials for SpendLore",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "run": "bun",
    "test": "vitest run",
    "test:watch": "vitest",
    "site:dev": "cd site && npm run dev",
    "site:sync": "cd site && node scripts/sync-files.js",
    "site:build": "cd site && npm run build"
  },
  "dependencies": {
    "effect": "^3.19.15",
    "@effect/platform": "^0.94.2",
    "@effect/platform-node": "^0.104.1",
    "@effect/sql": "^0.49.0",
    "@effect/sql-pg": "^0.50.1"
  },
  "devDependencies": {
    "@effect/vitest": "^0.27.0",
    "@types/bun": "latest",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
